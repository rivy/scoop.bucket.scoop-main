        }
    },
    "extract_dir": "Rust",
    "bin": [
        "bin\\rustc.exe",
        "bin\\rustdoc.exe",
        "bin\\cargo.exe"
    ],
    "checkver": {
        "url": "https://www.rust-lang.org/",
        "regex": "Version ([\\d.]+)"
    },
    "autoupdate": {
