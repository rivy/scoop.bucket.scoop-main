            "64bit": {
                "url": "https://static.rust-lang.org/dist/rust-$version-x86_64-pc-windows-gnu.msi"
            },
            "32bit": {
                "url": "https://static.rust-lang.org/dist/rust-$version-i686-pc-windows-gnu.msi"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
