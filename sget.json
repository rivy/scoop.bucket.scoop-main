{
    "version": "1.12.1",
    "description": "Safer, automatic verification of signatures and integration.",
    "homepage": "https://github.com/sigstore/cosign#sget",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/sigstore/cosign/releases/download/v1.12.1/sget-windows-amd64.exe#/sget.exe",
            "hash": "cf5093c4328d29d4166a78e65cd8fa63ce75f2eeb35d98207ddfa5be21270b98"
        }
    },
    "bin": "sget.exe",
    "checkver": {
        "url": "https://github.com/sigstore/cosign/releases",
        "regex": "tree\\/v([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
