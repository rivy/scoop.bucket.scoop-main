    "architecture": {
        "64bit": {
            "url": "https://github.com/version-fox/vfox/releases/download/v0.3.4/vfox_0.3.4_windows_x86_64.zip",
            "hash": "61b5f71ac26fe311eed38c89eb617781871853b9e45c49bec060d31e4ae7eebd",
            "extract_dir": "vfox_0.3.4_windows_x86_64"
        },
        "32bit": {
