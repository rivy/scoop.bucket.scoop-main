            },
            "32bit": {
                "url": "https://dl.influxdata.com/telegraf/releases/telegraf-$version_windows_i386.zip"
            }
        },
        "hash": {
            "url": "https://github.com/influxdata/telegraf/releases/tag/v$version",
            "regex": "(?s)$basename</.*?<code>$sha256</"
        },
        "extract_dir": "telegraf-$version"
    }
